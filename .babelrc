{
    "presets": [
        "react",
        [
            "env",
            {
                "target": {
                    "browsers": "last 2 versions"
                },
                "loose": false,
                "modules": false
            }

        ]
    ],
    "plugins": [
        "react-hot-loader/babel",
        "babel-plugin-transform-class-properties"
    ],
    "env": {
        "test": {
            "plugins": ["babel-plugin-transform-es2015-modules-commonjs"]
        }
    }
}